<!doctype html>
<html class="touch-styles">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {.if template.mobileStylesEnabled}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {.end}
    {squarespace-headers}
    <script>
      (function(){
        var touchTest="ontouchstart"in window||navigator.msMaxTouchPoints;
        if(!touchTest){
          with(document.documentElement){
            className=className.replace(new RegExp("touch-styles"),"")
          }
        }
      })()
    </script>
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <div id="siteWrapper" class="clearfix">

      <div id="nine-template">
        <main id="page" role="main">
          <div id="content"
               class="main-content blk__sections"
               data-content-field="main-content"
               data-collection-id="{collection.id}"
               {.equal? collection.typeName "index"}{.or}data-edit-main-image="Banner"{.end}>
            
            <div class="blk__seed" aria-hidden="true"></div>

            <!-- 9 sections -->
            <section class="blk__section"><squarespace:block-field id="Section-01" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-02" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-03" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-04" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-05" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-06" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-07" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-08" columns="12" /></section>
            <section class="blk__section"><squarespace:block-field id="Section-09" columns="12" /></section>

          </div>
        </main>
      </div>

    </div>
    {squarespace-footers}
  </body>
</html>
