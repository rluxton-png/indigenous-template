<!doctype html>
<html class="touch-styles">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {.if template.mobileStylesEnabled}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {.end}
    {squarespace-headers}

    <!-- Temporary minimal responsive helpers; will move to .less when you share it -->
    <style>
      /* Fluid media: keeps iframes/videos/images inside their container */
      .project-media { width: 100%; }
      .project-media img,
      .project-media video { max-width: 100%; height: auto; display: block; }

      /* The wrapper JS adds; preserves a 16:9 (can be overridden in .less) */
      .embed-fluid { position: relative; width: 100%; }
      .embed-fluid::before { content: ""; display: block; padding-top: 56.25%; }
      .embed-fluid > iframe,
      .embed-fluid > video { position: absolute; inset: 0; width: 100%; height: 100%; border: 0; }

      /* Scalable headlines: gentle, elegant sizing (we’ll fine-tune in .less) */
      .section-header h1,
      .project-title h1 { font-size: clamp(1.75rem, 4vw, 3rem); line-height: 1.15; }
      .section-header h2 { font-size: clamp(1.25rem, 2.8vw, 2rem); line-height: 1.2; }
      .project-summary { max-width: 78ch; }
      .project-summary p { font-size: clamp(1rem, 1.4vw, 1.125rem); line-height: 1.55; }

      /* Prevent overflow on narrow viewports */
      .internal-wrapper { overflow-wrap: anywhere; }
    </style>

    <script>(function(){var t="ontouchstart"in window||navigator.msMaxTouchPoints;if(!t){with(document.documentElement){className=className.replace(new RegExp("touch-styles"),"")}}})();</script>
  </head>
  {.if template.mobileStylesEnabled}{.or}<!-- Mobile Styles Disabled -->{.end}
