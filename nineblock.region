    <!-- Tiny helper to wrap embeds responsively -->
    <script>
      (function () {
        function wrapEmbeds(scope) {
          var containers = (scope || document).querySelectorAll('.project-media, .responsive-media');
          containers.forEach(function (c) {
            c.querySelectorAll('iframe, video').forEach(function (el) {
              if (el.closest('.embed-fluid')) return;
              var w = document.createElement('div');
              w.className = 'embed-fluid';
              el.parentNode.insertBefore(w, el);
              w.appendChild(el);
            });
          });
        }
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', function(){ wrapEmbeds(); });
        } else { wrapEmbeds(); }

        // In case Squarespace lazy-loads blocks after initial render:
        document.addEventListener('ss-async-block-loaded', function(e){ wrapEmbeds(e.target); });
      })();
    </script>

