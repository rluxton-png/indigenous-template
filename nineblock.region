                    <squarespace:block-field id="Project-Media-{collection.id}-17" columns="12" />
                  </div>
                  <div class="project-summary">
                    <div class="summary-seperator"></div>
                    <div class="summary-text">
                      <squarespace:block-field id="Project-Summary-Text-{collection.id}-17" />
                    </div>
                  </div>
                </div>
              </div>

              <!-- Section 6 -->
              <div class="project-content">
                <div class="internal-wrapper">
                  <div class="section-header">
                    <squarespace:block-field id="Project-SectionHeader-{collection.id}-18" columns="12" />
                  </div>
