<!doctype html>
<html class="touch-styles">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {.if template.mobileStylesEnabled}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {.end}
    {squarespace-headers}
    <script>/* Must be below {squarespace-headers} */(function(){var touchTest="ontouchstart"in window||navigator.msMaxTouchPoints;if(!touchTest){with(document.documentElement){className=className.replace(new RegExp("touch-styles"),"")}}})()</script>
  </head>
  {.if template.mobileStylesEnabled}{.or}<!-- Mobile Styles Disabled -->{.end}
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}{.section website}{.if logoImageUrl} logo-image{.end}{.end}{.section collection} collection-{urlId|slugify}{.end}">

    <div id="site">

      <input type="checkbox" name="mobile-nav-toggle" id="mobileNavToggle" class="mobile-nav-toggle-box hidden" />

      <div id="overlayNav">
        <squarespace:navigation navigationId="newNav" template="mobile-navigation" />
      </div>

      <header id="project-header" role="banner">
        <div class="header-inner">
          <div class="header-left">
            <div class="header-logo">
              <a href="http://www.indigenousmedia.com"><img src="../assets/img/ind_logo_big.png" /></a>
            </div>
          </div>
          <div class="header-right">
            <div class="tmpl-loading"><squarespace:navigation navigationId="newNav" template="navigation" /></div>
          </div>
          <label for="mobileNavToggle" class="mobile-nav-toggle-label show-on-scroll-mobile"><div class="top-bar"></div><div class="middle-bar"></div><div class="bottom-bar"></div></label>
        </div>
      </header>

      <div id="siteWrapper" class="clearfix">

        <!-- Begin Nine-Section Layout (inherits Brand styles) -->
        <div id="brand-template" class="nine-template">
          <main id="page" role="main">
            <div id="content" class="main-content" data-content-field="main-content" data-collection-id="{collection.id}" {.equal? collection.typeName "index"}{.or}data-edit-main-image="Banner"{.end}>

              <!-- tiny seed so SS doesn't collapse when empty -->
              <div class="blk__seed" aria-hidden="true">&#160;</div>

              <!-- HERO / PROJECT HEADER (matches brand/blackrock) -->
              <div class="project-header">
                <div class="internal-wrapper">
                  <div class="project-header-left">
                    <div class="project-logos">
                      <div class="client-logo">
                        <squarespace:block-field id="NB-Client-Logo-{collection.id}" columns="12" />
                      </div>
                      <div class="ssd-logo">
                        <squarespace:block-field id="NB-SSD-Logo-{collection.id}" columns="12" />
                      </div>
                    </div>
                    <div class="project-title">
                      <squarespace:block-field id="NB-Project-Title-{collection.id}" columns="12" />
                    </div>
                  </div>
                  <div class="project-header-right">
                    <div class="project-awards">
                      <squarespace:block-field id="NB-Project-Awards-{collection.id}" columns="12" />
                    </div>
                  </div>
                </div>
              </div>

              <!-- Section 1 -->
              <div class="project-content">
                <div class="internal-wrapper">
                  <div class="section-header">
                    <squarespace:block-field id="Project-SectionHeader-{collection.id}-13" columns="12" />
                  </div>
                  <div class="project-media">
                    <squarespace:block-field id="Project-Media-{collection.id}-13" columns="12" />
                  </div>
                  <div class="project-summary">
                    <div class="summary-seperator"></div>
                    <div class="summary-text">
                      <squarespace:block-field id="Project-Summary-Text-{collection.id}-13" />
                    </div>
                  </div>
                </div>
              </div>

              <!-- Section 2 -->
              <div class="project-content">
                <div class="internal-wrapper">
                  <div class="section-header">
                    <squarespace:block-field id="Project-SectionHeader-{collection.id}-14" columns="12" />
                  </div>
                  <div class="project-media">
                    <squarespace:block-field id="Project-Media-{collection.id}-14" columns="12" />
                  </div>
                  <div class="project-summary">
                    <div class="summary-seperator"></div>
                    <div class="summary-text">
                      <squarespace:block-field id="Project-Summary-Text-{collection.id}-14" />
                    </div>
                  </div>
                </div>
              </div>

              <!-- Section 3 -->
              <div class="project-content">
                <div class="internal-wrapper">
                  <div class="section-header">
                    <squarespace:block-field id="Project-SectionHeader-{collection.id}-15" columns="12" />
                  </div>
                  <div class="project-media">
                    <squarespace:block-field id="Project-Media-{collection.id}-15" columns="12" />
                  </div>
                  <div class="project-summary">
                    <div class="summary-seperator"></div>
                    <div class="summary-text">
                      <squarespace:block-field id="Project-Summary-Text-{collection.id}-15" />
                    </div>
