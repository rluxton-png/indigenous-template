<!doctype html>
<html class="touch-styles">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {.if template.mobileStylesEnabled}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {.end}
    {squarespace-headers}
    <script>(function(){var t="ontouchstart"in window||navigator.msMaxTouchPoints;if(!t){with(document.documentElement){className=className.replace(new RegExp("touch-styles"),"")}}})();</script>
  </head>
  {.if template.mobileStylesEnabled}{.or}<!-- Mobile Styles Disabled -->{.end}
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}{.section website}{.if logoImageUrl} logo-image{.end}{.end}{.section collection} collection-{urlId|slugify}{.end}">

    <div id="site">
      <input type="checkbox" name="mobile-nav-toggle" id="mobileNavToggle" class="mobile-nav-toggle-box hidden" />

      <div id="overlayNav">
        <squarespace:navigation navigationId="newNav" template="mobile-navigation" />
      </div>

      <header id="project-header" role="banner">
        <div class="header-inner">
          <div class="header-left">
            <div class="header-logo">
              <a href="http://www.indigenousmedia.com"><img src="../assets/img/ind_logo_big.png" /></a>
            </div>
          </div>
          <div class="header-right">
            <div class="tmpl-loading"><squarespace:navigation navigationId="newNav" template="navigation" /></div>
          </div>
          <label for="mobileNavToggle" class="mobile-nav-toggle-label show-on-scroll-mobile">
            <div class="top-bar"></div><div class="middle-bar"></div><div class="bottom-bar"></div>
          </label>
        </div>
      </header>

      <div id="siteWrapper" class="clearfix">

        <!-- Inherit Brand styles -->
        <div id="brand-template" class="nine-template">
          <main id="page" role="main">
            <div id="content" class="main-content" data-content-field="main-content" data-collection-id="{collection.id}" {.equal? collection.typeName "index"}{.or}data-edit-main-image="Banner"{.end}>

              <!-- seed so empty pages still render -->
              <div class="blk__seed" aria-hidden="true">&#160;</div>

              <!-- HERO — uses SAME IDs as Brand/BlackRock -->
              <div class="project-header">
                <div class="internal-wrapper">
                  <div class="project-header-left">
                    <div class="project-logos">
                      <div class="client-logo"><squarespace:block-field id="Client-Logo-{collection.id}" columns="12" /></div>
                      <div class="ssd-logo"><squarespace:block-field id="60sd-Logo-{collection.id}" columns="12" /></div>
                    </div>
                    <div class="project-title">
                      <squarespace:block-field id="Project-Title-{collection.id}" columns="12" />
                    </div>
                  </div>
                  <div class="project-header-right">
                    <div class="project-awards">
                      <squarespace:block-field id="Project-Awards-{collection.id}" columns="12" />
                    </div>
                  </div>
                </div>
              </div>

              <!-- ===== FIRST SIX SECTIONS — EXACT SAME IDS & ORDER AS BRAND TEMPLATE ===== -->
              <!-- order: 6, 4, 1, 2, 5, 3 (matches brandtemplate.region) -->

              <div class="project-content"><div class="internal-wrapper">
                <!-- Use Heading 1 for largest; Heading 2 anywhere in the section renders as mid-display (H1.5) -->
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-6" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-6" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-6" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-4" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-4" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-4" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-1" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-1" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-1" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-2" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-2" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-2" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-5" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-5" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-5" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-3" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-3" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-3" /></div>
                </div>
              </div></div>

              <!-- ===== EXTRA 3 SECTIONS — NEW IDS SO THEY DON'T CONFLICT ===== -->

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-7" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-7" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-7" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-8" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-8" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-8" /></div>
                </div>
              </div></div>

              <div class="project-content"><div class="internal-wrapper">
                <div class="section-header">
                  <squarespace:block-field id="Project-SectionHeader-{collection.id}-9" columns="12" />
                </div>
                <div class="project-media"><squarespace:block-field id="Project-Media-{collection.id}-9" columns="12" /></div>
                <div class="project-summary">
                  <div class="summary-seperator"></div>
                  <div class="summary-text"><squarespace:block-field id="Project-Summary-Text-{collection.id}-9" /></div>
                </div>
              </div></div>

            </div>
          </main>
        </div>
        <!-- End Nine-Section Layout -->

        <footer id="footer" role="contentinfo">
          <div class="footer-inner">
            <div class="footer-left">
              <div class="footer-logo">
                <a href="http://www.indigenousmedia.com"><img src="../assets/img/IND_logo_2.png" /></a>
              </div>
              <div class="footer-links">
                <ul>
                  <li><a href="https://www.indigenousmedia.com/#original-content-section-section">Original Content</a></li>
                  <li><a href="https://www.indigenousmedia.com/#brand-content-section-section">For Brands</a></li>
                  <li><a href="https://www.indigenousmedia.com/privacy-terms">Privacy & Terms</a></li>
                </ul>
              </div>
              <div class="footer-email">
                <squarespace:block-field id="footerEmail" columns="12" label="Footer Email Signup" />
              </div>
            </div>
            <div class="footer-right">
              <div class="footer-social">
                <strong>Follow Us on Social Media</strong>
                <div>
                  <squarespace:block-field id="footerSocial" label="Footer Social Links" />
                </div>
              </div>
            </div>
          </div>
        </footer>

      </div>
    </div>

    <squarespace:script src="no-yui.js" combo="true" />
    <squarespace:script src="no-hover-on-scroll.js" combo="true" />
    <squarespace:script src="lazyload.js" combo="true" />
    <squarespace:script src="sticky.js" combo="true" />
    <squarespace:script src="centernav.js" combo="true" />
    <squarespace:script src="index-gallery.js" combo="true" />
    <squarespace:script src="site.js" combo="true" />
    <squarespace:script src="helpers.js" combo="true" />
    <squarespace:script src="authenticated.js" combo="true" />
    <script src="//code.jquery.com/jquery-2.2.1.min.js"></script>
    <squarespace:script src="jquery.modal.min.js" />
    <squarespace:script src="fb.js" combo="true" />
    <script src="https://apis.google.com/js/platform.js"></script>

  </body>
</html>
